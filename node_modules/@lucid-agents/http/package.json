{
  "name": "@lucid-agents/http",
  "version": "1.9.7",
  "description": "HTTP extension for agent runtime",
  "repository": {
    "type": "git",
    "url": "https://github.com/daydreamsai/lucid-agents",
    "directory": "packages/http"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@lucid-agents/types": "^1.5.6",
    "hono": "4.10.1"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "zod": "^4.1.12"
  }
}
