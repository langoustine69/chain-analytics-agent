{
  "name": "@lucid-agents/core",
  "version": "2.1.0",
  "description": "Build typed agent HTTP apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/daydreamsai/lucid-agents",
    "directory": "packages/core"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./axllm": {
      "types": "./dist/axllm/index.d.ts",
      "import": "./dist/axllm/index.js",
      "default": "./dist/axllm/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "AGENTS.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "zod": "^4.1.12",
    "hono": "4.10.1",
    "@lucid-dreams/agent-auth": "latest",
    "@lucid-dreams/client": "latest",
    "@lucid-agents/types": "^1.5.6",
    "@lucid-agents/a2a": "^0.5.4",
    "@lucid-agents/payments": "^2.1.0",
    "x402-fetch": "^0.7.3",
    "x402": "^0.7.3",
    "viem": "^2.41.2",
    "@ax-llm/ax": "^14.0.37",
    "@ax-llm/ax-tools": "^14.0.37"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "@lucid-agents/eslint-config": "^1.0.0",
    "@lucid-agents/prettier-config": "^1.0.0",
    "@lucid-agents/analytics": "^0.2.5",
    "@lucid-agents/ap2": "^0.3.7",
    "@lucid-agents/http": "^1.9.7",
    "@lucid-agents/identity": "^2.1.0",
    "@lucid-agents/scheduler": "^0.1.4",
    "@lucid-agents/wallet": "^0.5.6"
  }
}
