{
  "name": "@lucid-agents/hono",
  "version": "0.7.13",
  "description": "Hono adapter for Lucid agent runtime",
  "repository": {
    "type": "git",
    "url": "https://github.com/daydreamsai/lucid-agents",
    "directory": "packages/hono"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
