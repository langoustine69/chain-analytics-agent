  },
  "dependencies": {
    "@lucid-agents/ap2": "^0.3.7",
    "@lucid-agents/core": "^2.1.0",
    "@lucid-agents/payments": "^2.1.0",
    "@lucid-agents/types": "^1.5.6",
    "hono": "4.10.1",
    "x402-hono": "^0.7.1",
    "x402": "^0.7.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@lucid-agents/http": "^1.9.7",
    "@lucid-agents/wallet": "^0.5.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
